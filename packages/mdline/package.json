{
  "name": "mdline",
  "version": "1.0.0",
  "description": "markdown to timeline tool.",
  "keywords": [
    "cli",
    "mdline"
  ],
  "homepage": "https://github.com/azu/mdline/tree/master/packages/mdline/",
  "bugs": {
    "url": "https://github.com/azu/mdline/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/azu/mdline.git"
  },
  "license": "MIT",
  "author": "azu",
  "files": [
    "bin/",
    "lib/",
    "src/"
  ],
  "main": "lib/mdline.js",
  "types": "lib/mdline.d.ts",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production tsc -p .",
    "clean": "rimraf lib/",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,css}\"",
    "prepublish": "npm run --if-present build",
    "test": "mocha \"test/**/*.ts\"",
    "watch": "tsc -p . --watch"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": false,
    "tabWidth": 4
  },
  "dependencies": {
    "@babel/generator": "^7.2.2",
    "@babel/parser": "^7.2.3",
    "@mdline/mdline-formatter-html": "^1.0.0",
    "@mdline/mdline-parser": "^1.0.0",
    "meow": "^5.0.0"
  },
  "devDependencies": {
    "@types/babel__generator": "^7.0.1",
    "@types/meow": "^5.0.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.18",
    "cross-env": "^5.2.0",
    "mocha": "^5.2.0",
    "prettier": "^1.15.3",
    "rimraf": "^2.6.3",
    "ts-node": "^7.0.1",
    "ts-node-test-register": "^4.0.0",
    "typescript": "^3.2.2"
  }
}
