{
  "name": "@mdline/mdline-formatter-html",
  "version": "1.0.1",
  "description": "HTML formatter for mdline",
  "keywords": [
    "mdline"
  ],
  "homepage": "https://github.com/azu/mdline/tree/master/packages/mdline-formatter-html/",
  "bugs": {
    "url": "https://github.com/azu/mdline/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/azu/mdline.git"
  },
  "license": "MIT",
  "author": "azu",
  "files": [
    "bin/",
    "lib/",
    "src/"
  ],
  "main": "lib/mdline-formatter-html.js",
  "types": "lib/mdline-formatter-html.d.ts",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production tsc -p .",
    "clean": "rimraf lib/",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,css}\"",
    "prepublish": "npm run --if-present build",
    "watch": "tsc -p . --watch"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": false,
    "tabWidth": 4
  },
  "dependencies": {
    "@mdline/types": "^1.0.1",
    "svelte": "^2.16.0"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.18",
    "cross-env": "^5.2.0",
    "mocha": "^5.2.0",
    "prettier": "^1.15.3",
    "rimraf": "^2.6.3",
    "ts-node": "^7.0.1",
    "ts-node-test-register": "^4.0.0",
    "typescript": "^3.2.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
